extends test-layout

block test-content
	h1 user

	.panel.panel-primary
		.panel-heading
			.panel-title retrive
		.panel-body
			form(action='retrive',method='post')
				form
					button.btn.btn-default(type='submit') Retrive All

	.panel.panel-default
		.panel-heading
			.panel-title output
		.panel-body
			ul
				each user in users
					li
						strong _id:
						span= user._id
						br
						strong email:
						span= user.email
						br
						strong password:
						span= user.password
						br
	